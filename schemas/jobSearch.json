{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "minSalary",
        "hasEquity"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Cashier"
            ]
        },
        "minSalary": {
            "type": "integer",
            "default": 0,
            "title": "The minSalary Schema",
            "examples": [
                40000
            ]
        },
        "hasEquity": {
            "type": "boolean",
            "default": false,
            "title": "The hasEquity Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "title": "Cashier",
        "minSalary": 40000,
        "hasEquity": true
    }]
}